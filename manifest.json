{
    "manifest_version": 3,
    "name": "AgePrediction",
    "description": "Base Level Extension",
    "version": "1.0",
    "host_permissions": [
      "http://localhost:5000/",
      "http://localhost/*",
      "https://localhost/*",
      "http://127.0.0.1/*",
      "https://127.0.0.1/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "content_scripts": [{
        "matches": [],
        "js": ["popup.js"]
      }]
    }
}